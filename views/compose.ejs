<%- include('./partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
        <label for="TitleInput">Title</label>
        <input  class="form-control" id="TitleInput" type="text" name="title">
    </div>
    <div class="form-group">
        <label for="TextInput">Post</label>
        <textarea class="form-control" id="TextInput" rows="5" name="content"></textarea>
    </div>
    
    <button type="submit" class="btn btn-primary mb-2">Publish</button>
</form> 

 <%- include('./partials/footer'); -%>